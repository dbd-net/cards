{
    "name": "gamebetr/cards",
    "description": "Extends probable library for card games",
    "authors": [
        {
            "name": "Steve Harmeyer",
            "email": "sharmeyer@gamebetr.com"
        },
        {
            "name": "Steven Wichers",
            "email": "me@stevenwichers.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-intl": "*",
        "gamebetr/provable": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "platform": {
            "php": "7.2"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gamebetr\\Cards\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gamebetr\\Cards\\Tests\\": "tests/"
        }
    }
}
